  "description": "Interactive command line interface to export CSV files from an Excel workbook",
  "scripts": {
    "start": "tsc && node build/index.js",
    "lint": "npm run lint:eslint && npm run lint:prettier",
    "lint:eslint": "eslint --fix 'src/**/*.ts'",
    "lint:prettier": "prettier --write src"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/sophiamersmann/xlsx-to-csv-cli.git"
  },
  "author": "Sophia Mersmann",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/sophiamersmann/xlsx-to-csv-cli/issues"
  },
  "homepage": "https://github.com/sophiamersmann/xlsx-to-csv-cli#readme",
  "dependencies": {
    "chalk": "^4.1.2",
    "inquirer": "^8.2.0",
    "inquirer-fuzzy-path": "^2.3.0",
    "xlsx": "^0.17.3"
  },
  "devDependencies": {
    "@types/inquirer": "^8.1.3",
    "@types/inquirer-fuzzy-path": "^2.3.3",
    "@types/node": "^16.11.5",
    "@typescript-eslint/eslint-plugin": "^5.2.0",
    "@typescript-eslint/parser": "^5.2.0",
    "eslint": "^8.1.0",
    "eslint-config-prettier": "^8.3.0",
    "prettier": "2.4.1",
    "typescript": "^4.4.4"
  }
